{
  "npmScope": "api",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "database": {
      "tags": []
    },
    "flow-flow-admin-storefront": {
      "tags": []
    },
    "flow-flow-auth": {
      "tags": []
    },
    "flow-flow-events": {
      "tags": []
    },
    "flow-flow-key-cycler": {
      "tags": []
    },
    "flow-flow-nft": {
      "tags": []
    },
    "flow-flow-service": {
      "tags": []
    },
    "flow-flow-storefront": {
      "tags": []
    },
    "flow-listener": {
      "tags": []
    },
    "logger": {
      "tags": []
    },
    "server": {
      "tags": []
    },
    "utils": {
      "tags": []
    }
  }
}
