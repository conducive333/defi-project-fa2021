{
  "name": "api",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "nx": "nx",
    "start": "cross-env NODE_ENV=development nx serve",
    "client:start": "node ./dist/apps/client-server/main.js",
    "admin:start": "node ./dist/apps/admin-server/main.js",
    "cron:start": "node ./dist/apps/cron-cleanup/main.js",
    "nft:start": "node ./dist/apps/flow-listener/main.js",
    "storefront:start": "node ./dist/apps/flow-listener/main.js",
    "build": "nx build",
    "build:stag": "cross-env NODE_ENV=staging nx build",
    "build:prod": "cross-env NODE_ENV=production nx build",
    "test": "cross-env NODE_ENV=testing nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "typeorm": "ts-node --transpile-only --project ./libs/database/tsconfig.lib.json ./node_modules/typeorm/cli.js",
    "entity:create": "cross-env NODE_ENV=development typeorm entity:create -d libs/database/src/entity -n ",
    "drop:dev": "cross-env NODE_ENV=development npm run typeorm schema:drop",
    "drop:test": "cross-env NODE_ENV=testing npm run typeorm schema:drop",
    "drop:stag": "cross-env NODE_ENV=staging npm run typeorm schema:drop",
    "drop:prod": "cross-env NODE_ENV=production npm run typeorm schema:drop",
    "migrate:make": "typeorm migration:create -d libs/database/src/migration -n ",
    "migrate:gen": "cross-env NODE_ENV=development npm run typeorm migration:generate -- -n",
    "migrate:dev": "cross-env NODE_ENV=development npm run typeorm migration:run",
    "rollback:dev": "cross-env NODE_ENV=development npm run typeorm migration:revert",
    "migrate:test": "cross-env NODE_ENV=testing npm run typeorm migration:run",
    "rollback:test": "cross-env NODE_ENV=testing npm run typeorm migration:revert",
    "migrate:stag": "cross-env NODE_ENV=staging npm run typeorm migration:run",
    "rollback:stag": "cross-env NODE_ENV=staging npm run typeorm migration:revert",
    "migrate:prod": "cross-env NODE_ENV=production npm run typeorm migration:run",
    "rollback:prod": "cross-env NODE_ENV=production npm run typeorm migration:revert",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "dependencies": {
    "@nestjs/common": "8.0.8",
    "@nestjs/config": "1.0.1",
    "@nestjs/core": "8.0.8",
    "@nestjs/passport": "8.0.1",
    "@nestjs/platform-express": "8.0.8",
    "@nestjs/schedule": "1.0.1",
    "@nestjs/swagger": "5.1.0",
    "@nestjs/typeorm": "8.0.2",
    "@onflow/fcl": "0.0.77",
    "@onflow/sdk": "0.0.54",
    "@onflow/types": "0.0.5",
    "@paypal/checkout-server-sdk": "1.0.3",
    "aws-sdk": "2.996.0",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "compression": "1.7.4",
    "elliptic": "6.5.4",
    "express": "4.17.1",
    "express-basic-auth": "1.2.0",
    "express-session": "1.17.2",
    "fcl-kms-authorizer": "0.0.7",
    "helmet": "4.6.0",
    "joi": "17.4.2",
    "nodemailer": "6.6.5",
    "passport": "0.5.0",
    "passport-local": "1.0.0",
    "pg": "8.7.1",
    "rate-limiter-flexible": "2.3.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.3.0",
    "sha3": "2.1.4",
    "swagger-ui-express": "4.1.6",
    "tslib": "2.3.1",
    "typeorm": "0.2.37",
    "typeorm-store": "2.0.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@nestjs/schematics": "8.0.3",
    "@nestjs/testing": "8.0.8",
    "@nrwl/cli": "12.9.0",
    "@nrwl/eslint-plugin-nx": "12.9.0",
    "@nrwl/jest": "12.9.0",
    "@nrwl/linter": "12.9.0",
    "@nrwl/nest": "12.9.0",
    "@nrwl/node": "12.9.0",
    "@nrwl/tao": "12.9.0",
    "@nrwl/workspace": "12.9.0",
    "@types/compression": "1.7.2",
    "@types/cron": "1.7.3",
    "@types/elliptic": "6.4.13",
    "@types/express": "4.17.13",
    "@types/express-session": "1.17.4",
    "@types/faker": "5.5.8",
    "@types/jest": "27.0.2",
    "@types/multer": "1.4.7",
    "@types/node": "16.10.1",
    "@types/nodemailer": "6.4.4",
    "@types/passport-local": "1.0.34",
    "@types/pg": "8.6.1",
    "@types/swagger-ui-express": "4.1.3",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "~4.28.3",
    "@typescript-eslint/parser": "~4.28.3",
    "cross-env": "7.0.3",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "faker": "5.5.3",
    "jest": "27.2.3",
    "prettier": "2.4.1",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "typescript": "4.4.3"
  }
}
