          "options": {
            "lintFilePatterns": ["libs/database/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/database"],
          "options": {
            "jestConfig": "libs/database/jest.config.js",
            "passWithNoTests": true
