    "flow-listener": {
      "root": "apps/flow-listener",
      "sourceRoot": "apps/flow-listener/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/flow-listener",
            "main": "apps/flow-listener/src/main.ts",
            "tsConfig": "apps/flow-listener/tsconfig.app.json",
            "assets": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/flow-listener/src/environments/environment.ts",
                  "with": "apps/flow-listener/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "flow-listener:build"
